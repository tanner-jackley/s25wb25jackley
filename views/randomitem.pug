extends layout

block content
  h1= title
  - var image_names = ["yacht1.jpg", "yacht2.jpg", "yacht3.jpg", "yacht4.jpg", "yacht5.jpg"]
  - for (var i=0; i<3; i++) {
      - var rand_num = Math.floor(Math.random()*5)
      img(src=`images/${image_names[rand_num]}`, alt=`${image_names[rand_num]}`, height=300, width=300)
  - }